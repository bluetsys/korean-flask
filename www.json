{
    "admin": {
        "disabled": false,
        "enforce_origin": false,
        "origins": ["0.0.0.0:2019","localhost:2019"]
      },
    "apps": {
      "http": {
        "servers": {
          "srv0": {
            "listen": [
              ":80"
            ],
            "routes": [
              {
                "handle": [
                  {
                    "handler": "subroute",
                    "routes": [
                      {
                        "handle": [
                          {
                            "handler": "reverse_proxy",
                            "load_balancing": {
                              "selection_policy": {
                                "choose": 2,
                                "policy": "random_choose"
                              }
                            },
                            "upstreams": [
                              {
                                "dial": "korean-flask-core-app-1:5000"
                              },
                              {
                                "dial": "korean-flask-core-app-2:5000"
                              },
                              {
                                "dial": "korean-flask-core-app-3:5000"
                              },
                              {
                                "dial": "korean-flask-core-app-4:5000"
                              }
                            ]
                          }
                        ],
                        "match": [
                          {
                            "path": [
                              "/"
                            ]
                          }
                        ]
                      }
                    ]
                  }
                ],
                "match": [
                  {
                    "host": [
                      "localhost"
                    ]
                  }
                ],
                "terminal": true
              }
            ]
          }
        }
      }
    }
  }